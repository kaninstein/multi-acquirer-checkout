{
    "name": "kaninstein/multi-acquirer-checkout",
    "version": "1.0.2",
    "description": "Multi-acquirer checkout package with DDD architecture for Laravel",
    "keywords": [
        "laravel",
        "payment",
        "checkout",
        "ddd",
        "multi-acquirer",
        "pagarme",
        "stripe",
        "mercadopago",
        "appmax"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/events": "^10.0|^11.0|^12.0",
        "kaninstein/laravel-pagarme": "^1.0",
        "picqer/php-barcode-generator": "^3.2"
    },
    "suggest": {},
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "phpstan/phpstan": "^1.10",
        "orchestra/testbench": "^8.0|^9.0",
        "mockery/mockery": "^1.6",
        "laravel/pint": "^1.13"
    },
    "autoload": {
        "psr-4": {
            "Kaninstein\\MultiAcquirerCheckout\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kaninstein\\MultiAcquirerCheckout\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:coverage": "vendor/bin/phpunit --coverage-html coverage",
        "phpstan": "vendor/bin/phpstan analyse src --level=8",
        "format": "vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kaninstein\\MultiAcquirerCheckout\\MultiAcquirerCheckoutServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
